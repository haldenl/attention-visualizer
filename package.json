{
  "name": "attention-visualizer",
  "version": "0.0.1",
  "private": true,
  "description": "visualization tool for attention in seq2seq models",
  "repository": "https://github.com/haldenl/attention-visualizer.git",
  "author": "Halden Lin <haldenl@cs.washington.edu>",
  "license": "BSD-3-Clause",
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "webpack-dev-server --mode development --config webpack.dev.js --port 4000 --open",
    "build-js": "webpack --mode production --config webpack.prod.js",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "d3": "^5.4.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-json-view": "^1.17.0",
    "react-minimap": "^0.7.4",
    "react-rangeslider": "^2.2.0",
    "react-split-pane": "^0.1.77"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/d3": "^5.0.0",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.5",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "source-map": "^0.7.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.3.0",
    "typescript": "^2.9.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.10.2",
    "webpack-cli": "2.1.4",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  }
}
